<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="unqualified" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bc="www.kwantu.net/basecomponents/simpletype" xmlns:calc="http://www.kwantu.net/calc" vc:minVersion="1.1" xmlns:vc="http://www.w3.org/2007/XMLSchema-versioning">
	<xs:include schemaLocation="kwantu_base_types.xsd"/>
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="../shared/xml.xsd"/>
	<xs:import namespace="http://www.kwantumedia.com/2012/AtomExtension" schemaLocation="kwantu.xsd"/>
	<xs:import namespace="http://www.w3.org/2001/XMLSchema-instance" schemaLocation="xsi.xsd"/>
	<xs:import namespace="http://www.w3.org/2005/Atom" schemaLocation="ns1.xsd"/>
	<xs:import namespace="http://www.w3.org/XML/1998/namespace" schemaLocation="xml.xsd"/>
	<xs:element name="component">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="identification"/>
				<xs:element name="configuration">
					<xs:complexType>
						<xs:choice>
							<xs:element name="indicator">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="set" maxOccurs="unbounded">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="version">
														<xs:complexType>
															<xs:sequence>
																<xs:element ref="versionInfo"/>
																<xs:element name="settings">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="heading"/>
																			<xs:element name="languages"/>
																			<xs:element name="searchable" type="xs:boolean"/>
																			<xs:element name="includes" minOccurs="0">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="components"/>
																						<xs:element name="taxonomies"/>
																						<xs:element name="globalTemplates">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element maxOccurs="unbounded" name="globalTemplate">
																										<xs:complexType>
																											<xs:sequence/>
																											<xs:attribute ref="id" use="required"/>
																										</xs:complexType>
																									</xs:element>
																								</xs:sequence>
																							</xs:complexType>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element minOccurs="0" ref="reportDefaults"/>
																			<xs:element name="dataStore">
																				<xs:simpleType>
																					<xs:restriction base="xs:string">
																						<xs:enumeration value="profile"/>
																						<xs:enumeration value="userProfile"/>
																						<xs:enumeration value="communityProfile"/>
																					</xs:restriction>
																				</xs:simpleType>
																			</xs:element>
																			<xs:element ref="privacy"/>
																			<xs:element ref="uiTemplates"/>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="uiLayouts" type="kw_uiLayouts_type"> </xs:element>
																<xs:element name="rules">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element name="rule" maxOccurs="unbounded" minOccurs="0">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element maxOccurs="unbounded" name="action"/>
																						<xs:element minOccurs="0" name="updates">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element maxOccurs="unbounded" name="update">
																										<xs:complexType>
																											<xs:attribute name="source"/>
																											<xs:attribute name="target"/>
																										</xs:complexType>
																									</xs:element>
																								</xs:sequence>
																							</xs:complexType>
																						</xs:element>
																					</xs:sequence>
																					<xs:attribute ref="id" use="required"/>
																					<xs:attribute name="event" use="required">
																						<xs:simpleType>
																							<xs:restriction base="xs:NCName">
																								<xs:enumeration value="onCreate"/>
																								<xs:enumeration value="beforePersist"/>
																								<xs:enumeration value="afterPersist"/>
																								<xs:enumeration value="onClick"/>
																							</xs:restriction>
																						</xs:simpleType>
																					</xs:attribute>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																	</xs:complexType>
																</xs:element>
																<xs:element name="elements">
																	<xs:complexType>
																		<xs:group ref="indicatorSetElements"/>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute name="version" form="unqualified" use="required" type="xs:string"> </xs:attribute>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute ref="id" use="required"/>
												<xs:attribute ref="currentVersion" use="required"/>
												<xs:attribute ref="devVersion" use="required"/>
												<xs:attributeGroup ref="kw_inherited_element_source"/>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attributeGroup ref="kw_inherited_element_source"/>
									<xs:attribute ref="id" use="required"/>
								</xs:complexType>
							</xs:element>
							<xs:element name="performance">
								<xs:complexType>
									<xs:sequence>
										<xs:element name="settings" type="kw_component_header_type"/>
										<xs:element ref="uiTemplates"/>
										<xs:element name="sets">
											<xs:complexType>
												<xs:sequence>
													<xs:element name="set" maxOccurs="unbounded">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="version">
																	<xs:complexType>
																		<xs:sequence>
																			<xs:element ref="versionInfo"/>
																			<xs:element ref="settings"/>
																			<xs:element ref="uiTemplates" minOccurs="0"/>
																			<xs:element name="rules">
																				<xs:complexType>
																					<xs:sequence>
																						<xs:element name="rule" maxOccurs="unbounded" minOccurs="0">
																							<xs:complexType>
																								<xs:sequence>
																									<xs:element maxOccurs="unbounded" name="action"/>
																								</xs:sequence>
																								<xs:attribute ref="id" use="required"/>
																								<xs:attribute name="event" use="required">
																									<xs:simpleType>
																										<xs:restriction base="xs:NCName">
																											<xs:enumeration value="onCreate"/>
																											<xs:enumeration value="beforePersist"/>
																											<xs:enumeration value="afterPersist"/>
																										</xs:restriction>
																									</xs:simpleType>
																								</xs:attribute>
																							</xs:complexType>
																						</xs:element>
																					</xs:sequence>
																				</xs:complexType>
																			</xs:element>
																			<xs:element name="elements">
																				<xs:complexType>
																					<xs:group ref="performanceIndicatorSetElements" maxOccurs="unbounded" minOccurs="0"/>
																					<xs:attribute ref="id"/>
																					<xs:attributeGroup ref="kw_inherited_element_source"/>
																				</xs:complexType>
																			</xs:element>
																		</xs:sequence>
																		<xs:attribute name="version" form="unqualified" use="required" type="xs:string"> </xs:attribute>
																	</xs:complexType>
																</xs:element>
															</xs:sequence>
															<xs:attribute ref="id" use="required"/>
															<xs:attribute ref="currentVersion" use="required"/>
															<xs:attribute ref="devVersion" use="required"/>
															<xs:attribute name="setType" use="required">
																<xs:annotation>
																	<xs:documentation>Defines if this set shows as part of the global defaults, or only the node defaults</xs:documentation>
																</xs:annotation>
																<xs:simpleType>
																	<xs:restriction base="xs:string">
																		<xs:enumeration value="global"/>
																		<xs:enumeration value="node"/>
																	</xs:restriction>
																</xs:simpleType>
															</xs:attribute>
															<xs:attributeGroup ref="kw_inherited_element_source"/>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
									<xs:attributeGroup ref="kw_inherited_element_source"/>
								</xs:complexType>
							</xs:element>
						</xs:choice>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="identification">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="name"/>
				<xs:element ref="documentation"/>
			</xs:sequence>
			<xs:attribute ref="applicationId" use="required"/>
			<xs:attribute ref="baseComponentType" use="required"/>
			<xs:attribute name="id" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="versionInfo">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="publish">
					<xs:complexType>
						<xs:attribute name="date" type="xs:date"/>
					</xs:complexType>
				</xs:element>
				<xs:element ref="upgradeInformation"/>
				<xs:element ref="documentation"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="upgradeInformation" type="xs:string"> </xs:element>
	<xs:element name="settings" type="kw_performance_settings_type"> </xs:element>
	<xs:element name="heading" type="kw_i18n_type"/>
	<xs:element name="uiTemplates" type="kw_uiTemplates_type"> </xs:element>
	<xs:element name="privacy">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="add">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="roles"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="create">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="roles"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="read">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="roles"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="update">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="roles"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="delete">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="roles"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="documentation" type="kw_i18n_type"/>
	<xs:element name="namespace" type="xs:NCName"/>
	<xs:element name="IndicatorSetConfigData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="settings"/>
				<xs:element name="version">
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="versionInfo"/>
							<xs:element ref="settings"/>
							<xs:element name="elements">
								<xs:complexType>
									<xs:group ref="indicatorSetElements"/>
									<xs:attribute ref="id"/>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
						<xs:attribute name="version" form="unqualified" use="required">
							<xs:simpleType>
								<xs:restriction base="xs:string"/>
							</xs:simpleType>
						</xs:attribute>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="profileConfigData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="settings"/>
				<xs:element name="Elements">
					<xs:complexType>
						<xs:group ref="profileElements"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Version" use="required" type="xs:decimal"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="pageConfigData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="settings"/>
				<xs:element name="Elements">
					<xs:complexType>
						<xs:group ref="pageElements"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Version" use="required" type="xs:decimal"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="objectViewConfigData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="settings"/>
				<xs:element name="Elements">
					<xs:complexType>
						<xs:group ref="objectViewElements"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Version" use="required" type="xs:decimal"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="rendererConfigData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="settings"/>
				<xs:element name="Elements">
					<xs:complexType>
						<xs:group ref="rendererElements"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Version" use="required" type="xs:decimal"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="communityProfileConfigData">
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="settings"/>
				<xs:element name="Elements">
					<xs:complexType>
						<xs:group ref="communityProfileElements"/>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
			<xs:attribute name="Version" use="required" type="xs:decimal"/>
		</xs:complexType>
	</xs:element>
	<xs:group name="indicatorSetElements">
		<xs:sequence>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="elField" maxOccurs="unbounded"/>
				<xs:element maxOccurs="unbounded" name="elUpload" type="kw_elUpload_type"/>
				<xs:element ref="elSelect" maxOccurs="unbounded"/>
				<xs:element ref="elRepeat" maxOccurs="unbounded"/>
			</xs:choice>
		</xs:sequence>
	</xs:group>
	<xs:group name="performanceIndicatorSetElements">
		<xs:choice>
			<xs:element ref="elPerformance"/>
		</xs:choice>
	</xs:group>
	<xs:group name="profileElements">
		<xs:choice>
			<xs:element ref="label"/>
			<xs:element ref="elField"/>
			<xs:element ref="elSelect"/>
			<xs:element ref="elRepeat"/>
			<xs:element ref="elGroup"/>
			<xs:element ref="hierarchy"/>
			<xs:element ref="elSelectGroup"/>
		</xs:choice>
	</xs:group>
	<xs:group name="pageElements">
		<xs:choice>
			<xs:element ref="label"/>
			<xs:element ref="elField"/>
			<xs:element ref="elSelect"/>
			<xs:element ref="elRepeat"/>
			<xs:element ref="elGroup"/>
			<xs:element ref="hierarchy"/>
			<xs:element ref="elSelectGroup"/>
		</xs:choice>
	</xs:group>
	<xs:group name="objectViewElements">
		<xs:choice>
			<xs:element ref="label"/>
			<xs:element ref="elField"/>
			<xs:element ref="elSelect"/>
			<xs:element ref="elRepeat"/>
		</xs:choice>
	</xs:group>
	<xs:group name="rendererElements">
		<xs:choice>
			<xs:element ref="label"/>
			<xs:element ref="elField"/>
			<xs:element ref="elSelect"/>
			<xs:element ref="elRepeat"/>
			<xs:element ref="elGroup"/>
			<xs:element ref="hierarchy"/>
			<xs:element ref="elSelectGroup"/>
		</xs:choice>
	</xs:group>
	<xs:group name="communityProfileElements">
		<xs:choice>
			<xs:element ref="label"/>
			<xs:element ref="elField"/>
			<xs:element ref="elSelect"/>
			<xs:element ref="elRepeat"/>
			<xs:element ref="elGroup"/>
			<xs:element ref="hierarchy"/>
			<xs:element ref="elSelectGroup"/>
		</xs:choice>
	</xs:group>
	<xs:element name="reportDefaults" type="kw_ReportDefaults_type"> </xs:element>
	<xs:element name="tableName" type="xs:NCName"/>
	<xs:element name="elRepeat" type="kw_Repeat_type">
		<xs:annotation id="Repeat">
			<xs:documentation>Root element for the structure that defines the repeat or table structure</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:element name="elField" type="kw_elField_type"> </xs:element>
	<xs:element name="elSelect" type="kw_elSelect_type"> </xs:element>
	<xs:element name="elPageHeader"/>
	<xs:element name="elPageLayoutArea"/>
	<xs:element name="elDisplayView"/>
	<xs:element name="elActivityComment"/>
	<xs:element name="elListview"/>
	<xs:element name="elTagCloud"/>
	<xs:element name="elTagItem"/>
	<xs:element name="elHtmlEditor"/>
	<xs:element name="label" type="kw_label_type"/>
	<xs:element name="displays" type="kw_displays"/>
	<xs:element name="signature">
		<xs:simpleType>
			<xs:restriction base="xs:NCName">
				<xs:enumeration value="ColumnTotal"/>
				<xs:enumeration value="DateDiff"/>
				<xs:enumeration value="GetLocation"/>
				<xs:enumeration value="GetLattitude"/>
				<xs:enumeration value="GetLongitude"/>
				<xs:enumeration value="RowTotal"/>
				<xs:enumeration value="ElementValue"/>
				<xs:enumeration value="Sum"/>
				<xs:enumeration value="Product"/>
				<xs:enumeration value="Max"/>
				<xs:enumeration value="Average"/>
				<xs:enumeration value="SelectGroupKey"/>
			</xs:restriction>
		</xs:simpleType>
	</xs:element>
	<xs:element name="nodeCount" type="xs:integer"/>
	<xs:element name="nodes">
		<xs:complexType>
			<xs:sequence>
				<xs:element maxOccurs="unbounded" ref="node"/>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="node">
		<xs:complexType>
			<xs:simpleContent>
				<xs:extension base="xs:NCName">
					<xs:attribute name="action" use="required" type="xs:NCName"/>
					<xs:attribute name="seq" use="required" type="xs:integer"/>
				</xs:extension>
			</xs:simpleContent>
		</xs:complexType>
	</xs:element>
	<xs:element name="codelist">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Taxonomy">
					<xs:complexType>
						<xs:simpleContent>
							<xs:extension base="xs:NCName">
								<xs:attribute name="Type">
									<xs:simpleType>
										<xs:restriction base="xs:NCName">
											<xs:enumeration value="XSL"/>
											<xs:enumeration value="Rest"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:attribute>
							</xs:extension>
						</xs:simpleContent>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="help" type="kw_Help_type"> </xs:element>
	<xs:element name="reports" type="kw_Reports_type"> </xs:element>
	<xs:element name="column" type="reportColumnType"> </xs:element>
	<xs:element name="fieldName" type="xs:string"/>
	<xs:element name="repeatDef">
		<xs:annotation id="Repeat.RepeatDef">
			<xs:documentation>The RepeatDef defines the overall aspects of the repeat table such as how rows are added and what are the key elements</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="addRows">
					<xs:annotation id="AddRows">
						<xs:documentation>AddRows defines how the Repeat determines how many and which rows are added to the repeat table.</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:choice>
							<xs:element name="userAdd">
								<xs:annotation id="AddRows.UserAdd">
									<xs:documentation>If the UserAdd element is chosen, then the rows for the repeat table are added by the user selecting or adding them. The ways in which that can happen is defined in the child elements</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:sequence>
										<xs:element ref="add"/>
										<xs:element name="delete" type="kw_repeat_add_delete_type"/>
										<xs:element name="maxIterations" type="xs:short"/>
										<xs:element name="minIterations" type="xs:short"/>
										<xs:element name="addLinesMode" minOccurs="0">
											<xs:annotation>
												<xs:documentation>This might be deleted. Ignore for now</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:Name">
													<xs:enumeration value="BlankLines"/>
													<xs:enumeration value="UniqueLookup"/>
													<xs:enumeration value="SingleLookup"/>
													<xs:enumeration value="MultipleLookup"/>
													<xs:enumeration value="MultipleUniqueLookup"/>
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:sequence>
								</xs:complexType>
							</xs:element>
							<xs:element name="autoLoad">
								<xs:annotation id="AutoLoad">
									<xs:documentation>If the Autoload element is chosen, then the rows for the repeat table are added programmatically, and not by user choice. The ways in which that can happen is defined</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:choice>
										<xs:element name="blankLines" type="xs:short">
											<xs:annotation id="Addrows.BlankLines">
												<xs:documentation>Provide a number of blank rows as specified by the maxrows attribute</xs:documentation>
											</xs:annotation>
										</xs:element>
										<xs:element name="codelist">
											<xs:annotation id="AddRows.Codelist">
												<xs:documentation>Use the codelist specified by the KeyValue element to generate the rows.</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="Taxonomy" type="xs:NCName"/>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element name="periods">
											<xs:annotation id="AddRows.Periods">
												<xs:documentation>Adds rows according the number of periods between the start and end date, one for each period (or part of period for the first and last ones)	</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="StartDate">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Element" type="kw_Element_type"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
													<xs:element name="EndDate">
														<xs:complexType>
															<xs:sequence>
																<xs:element name="Element" type="kw_Element_type"/>
															</xs:sequence>
														</xs:complexType>
													</xs:element>
												</xs:sequence>
												<xs:attribute name="PeriodType" use="required">
													<xs:simpleType>
														<xs:restriction base="xs:Name">
															<xs:enumeration value="Month"/>
															<xs:enumeration value="Quarter"/>
															<xs:enumeration value="YearCalendar"/>
